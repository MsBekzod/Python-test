# Username Generator

import random

def user_name():
    name = input("Ismingizni kiriting: ")
    reversed_name = name[::-1]
    random_number = random.randint(0, 9)
    username = reversed_name + str(random_number)
    return username


print(user_name())






# String to Integers.

def convert_add(string_list):
    return sum(int(x) for x in string_list)
print(convert_add(['1', '3', '5']))

